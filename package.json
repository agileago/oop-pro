{
  "name": "starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --base /",
    "type:check": "tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{ts,tsx,js,vue,css,scss,html,json}'",
    "chrome": "open -n /Applications/Google\\ Chrome.app/ --args --disable-web-security  --user-data-dir=$HOME/MyChromeDevUserData/",
    "api": "tsg -c ./src/api/tsg.config.ts",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "@arco-design/web-vue": "^2.50.2",
    "@morev/vue-transitions": "^2.3.6",
    "@vueuse/core": "^10.3.0",
    "axios": "1.4.0",
    "expired-storage": "^1.0.2",
    "injection-js": "^2.4.0",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^6.2.1",
    "tailwind-variants": "^0.1.13",
    "ts-essentials": "^9.3.2",
    "tslib": "^2.6.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue3-oop": "^1.0.5"
  },
  "devDependencies": {
    "@arco-plugins/vite-vue": "^1.4.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/expired-storage": "^1.0.1",
    "@types/lodash": "^4.14.197",
    "@types/nprogress": "^0.2.0",
    "@types/prettier": "^2.7.2",
    "@types/swagger-schema-official": "^2.0.22",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vue3-oop/plugin-vue-jsx": "^1.4.6",
    "@vue3-oop/vite-plugin-html-template": "^1.1.6",
    "eslint": "~8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.28",
    "postcss-preset-env": "^9.1.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "sass": "^1.66.1",
    "simple-git-hooks": "^2.9.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-rem2px-preset": "^1.0.3",
    "terser": "^5.19.2",
    "ts-gear": "^4.11.7",
    "typescript": "^5.2.2",
    "vite": "4.4.9",
    "vite-plugin-ali-oss": "^1.0.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-mockit": "^1.0.2",
    "vite-svg-loader": "^4.0.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged",
    "commit-msg": "pnpm exec commitlint -e \"$@\""
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,css,scss,html,json}": [
      "prettier --write"
    ]
  }
}
